{
  "name": "prototype_commonModule",
  "version": "1.0.0",
  "description": "export and import examples using CommonJS and ES6 module approaches",
  "homepage": "github",
  "repository": "git@github.com:nischalsource/prototype_commonModule.git",
  "author": {
    "name": "Nischal Bachu",
    "email": "nischal.bachu@gmail.com",
    "url": ""
  },
  "files": [
    "src",
    "LICENSE"
  ],
  "main": "src/es6Modules_Index.js",
  "keywords": [
    "nodejs",
    "yeoman",
    "javascript"
  ],
  "scripts": {
    "clean": "rm -rf dist/*",
    "build": "npm run clean && babel src/commonJSmodules/* src/ES6modules/* src/*.js --out-dir dist/",
    "pretest": "eslint .",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@babel/node": "^7.13.10",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "babel-plugin-add-module-exports": "^1.0.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "coveralls": "^3.1.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^3.6.0",
    "eslint-config-xo": "^0.24.2",
    "eslint-plugin-prettier": "^2.6.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.2",
    "prettier": "^1.19.1"
  },
  "dependencies": {
    "esm": "^3.2.25"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      "add-module-exports",
      "transform-runtime"
    ]
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix"
    ],
    "*.json": [
      "prettier --write"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": [
      "prettier"
    ]
  },
  "jest": {
    "testEnvironment": "node"
  },
  "license": "MIT"
}
